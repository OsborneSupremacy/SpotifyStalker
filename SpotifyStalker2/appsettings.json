{
    "SpotifyApi": {
        "tokenUrl": "https://accounts.spotify.com/api/token",
        "spotifyBaseUrl": "https://api.spotify.com/v1",
        "apikeys": {
            "clientid": "storeInSecrets",
            "clientsecret": "storeInSecrets"
        },
        "limits": {
            "search": {
                "limit": 50,
                "maximumoffset": 1000
            },
            "userplaylist": 50,
            "playlisttrack": 100,
            "batchsize": 50
        }
    },
    "AllowedHosts": "*",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "Properties": {
            "Application": "Spotify Stalker"
        }
    }
}
